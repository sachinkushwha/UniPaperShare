<!DOCTYPE html>
<html lang="en">

<head>
  <meta name="description" content="AGCPYQP helps you prepare for exams by practicing previous year question papers online.">
  <meta name="keywords" content="AGCPYQP, previous year question papers, practice papers, online test">
  <meta name="author" content="sachin kushwaha">
  <meta name="google-site-verification" content="0AZXvz5XhCMKaRtmkmv1LppDr507uOzxnpZ946MgIUQ" />
  <!-- og (open graph) -->
  <meta property="og:title" content="Previous Year Question Papers | AGCPYQP" />
  <meta property="og:description" content="Practice previous year question papers online with AGCPYQP and get exam-ready." />
  <meta property="og:url" content="https://agcpyqp-1.onrender.com" />
  <meta property="og:type" content="website" />
  
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Previous Year Papers | PYQP</title>
    <script src="https://cdn.tailwindcss.com"></script>

  
</head>

<body class="bg-gray-100">
   
    <%-include('partials/nave')%>
        <div class="container  mx-auto p-6">
            <h1 class="text-2xl font-bold text-center mb-6 text-white">Previous Year Question Papers</h1>

            <% semester.forEach((sem)=>{%>
                <h2 class="text-2xl font-semibold mb-4 text-white"><%=sem%></h2>
                <div class="flex flex-wrap gap-6">
                    <% qpdata.forEach((data=>{ %>
                        <% if(data.semester===sem){%>
                        <div class="mb-6 ">
                            <div class="bg-black  bg-opacity-40 p-4 rounded-lg shadow-md ">
                                <h2 class="text-white font-semibold">Subject: <%=data.subject%>
                                </h2>
                                <p class="text-white">PaperType: <%=data.papertype%>
                                </p>
                                <p class="text-white">Year: <%=data.year%>
                                </p>
                                <div class="mt-2">
                                    <a href="/download/<%=data._id%>" 
                                        class="bg-blue-500 text-white px-4 py-2 rounded-lg mr-2">Download</a>
                                    <a href="/viewpaper/<%=data._id%>" target="_blank"
                                        class="bg-green-500 text-white px-4 py-2 rounded-lg">ðŸ“· View</a>
                                </div>
                            </div>
                        </div>
                        <%}%>
                        <%}))%>
                </div>
            <%})%>
               
        </div>
        <%-include('partials/footer')%>
</body>

</html>